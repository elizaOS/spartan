{
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "lib": [
      "ESNext",
      "dom"
    ],
    "target": "ESNext",
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": false,
    "forceConsistentCasingInFileNames": false,
    "allowImportingTsExtensions": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "allowJs": true,
    "checkJs": false,
    "noEmitOnError": false,
    "moduleDetection": "force",
    "allowArbitraryExtensions": true,
    "baseUrl": "../..",
    "typeRoots": [
      "../node_modules/@types"
    ],
    "paths": {
      "@elizaos/core": [
        "../core/src"
      ],
      "@elizaos/core/*": [
        "../core/src/*"
      ],
      "discord-api-types": [
        "../../node_modules/discord-api-types"
      ],
      "discord-api-types/*": [
        "../../node_modules/discord-api-types/*"
      ]
    }
  },
  "include": [
    "src/index.ts"
  ],
  "exclude": [
    "**/node_modules/@discordjs/builders/node_modules/**",
    "**/node_modules/@discordjs/formatters/node_modules/**",
    "**/node_modules/@discordjs/ws/node_modules/**",
    "**/node_modules/discord.js/typings/index.d.mts"
  ]
}